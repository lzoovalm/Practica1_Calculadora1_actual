        android:onClick="onClickButtonOne"
        android:text="1"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toTopOf="@+id/BottonFour"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/BottonTwo"
        android:layout_width="44dp"
        android:layout_height="40dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginEnd="39dp"
        android:layout_marginRight="39dp"
        android:layout_marginBottom="36dp"
        android:onClick="onClickButtonTwo"
        android:text="2"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toTopOf="@+id/BottonFive"
        app:layout_constraintEnd_toStartOf="@+id/BottonThree"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toEndOf="@+id/BottonOne" />

    <Button
        android:id="@+id/BottonThree"
        android:layout_width="43dp"
        android:layout_height="37dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginBottom="40dp"
        android:onClick="onClickButtonThree"
        android:text="3"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toTopOf="@+id/BottonSix"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/BottonFour"
        android:layout_width="46dp"
        android:layout_height="37dp"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginBottom="44dp"
        android:onClick="onClickButtonFour"
        android:text="4"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toTopOf="@+id/BottonSeven"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/BottonFive"
        android:layout_width="41dp"
        android:layout_height="38dp"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:layout_marginBottom="40dp"
        android:onClick="onClickButtonFive"
        android:text="5"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toTopOf="@+id/ButtonEight"
        app:layout_constraintEnd_toStartOf="@+id/BottonSix"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toEndOf="@+id/BottonFour" />

    <Button
        android:id="@+id/BottonSix"
        android:layout_width="44dp"
        android:layout_height="41dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginBottom="40dp"
        android:onClick="onClickButtonSix"
        android:text="6"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toTopOf="@+id/BottonNine"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/BottonSeven"
        android:layout_width="43dp"
        android:layout_height="34dp"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginBottom="440dp"
        android:onClick="onClickButtonSeven"
        android:text="7"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/ButtonEight"
        android:layout_width="41dp"
        android:layout_height="37dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:layout_marginBottom="36dp"
        android:onClick="onClickButtonEight"
        android:text="8"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toTopOf="@+id/BottonCero"
        app:layout_constraintEnd_toStartOf="@+id/BottonNine"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toEndOf="@+id/BottonSeven" />

    <Button
        android:id="@+id/BottonNine"
        android:layout_width="43dp"
        android:layout_height="37dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:layout_marginBottom="436dp"
        android:onClick="onClickButtonNine"
        android:text="9"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/BottonCero"
        android:layout_width="43dp"
        android:layout_height="35dp"
