        android:layout_marginRight="184dp"
        android:layout_marginBottom="364dp"
        android:onClick="onClickButtonCero"
        android:text="0"
        app:backgroundTint="#4B6587"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/Suma"
        android:layout_width="65dp"
        android:layout_height="88dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="155dp"
        android:layout_marginBottom="121dp"
        android:onClick="onClickSuma"
        android:text="+"
        app:backgroundTint="#C8C6C6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BottonSeven"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Divison"
        android:layout_width="62dp"
        android:layout_height="86dp"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginBottom="196dp"
        android:onClick="onClickDivision"
        android:text="/"
        app:backgroundTint="#C8C6C6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toEndOf="@+id/Multiplicacion" />

    <Button
        android:id="@+id/Resta"
        android:layout_width="62dp"
        android:layout_height="86dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="124dp"
        android:onClick="onClickResta"
        android:text="-"
        app:backgroundTint="#C8C6C6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Suma"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
